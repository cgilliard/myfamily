CC=gcc
CC_FLAGS=-g -Wno-format -Wno-attributes -Wno-ignored-attributes -I.. $(COVERAGE)
CRITERION_INCLUDE=-IREPLACE_CRITERION_INCLUDE
CRITERION_LIB=-LREPLACE_CRITERION_LIB
CC_TEST_FLAGS=-g -Wno-format -Wno-attributes -Wno-ignored-attributes -lcriterion -I.. $(CRITERION_INCLUDE) $(CRITERION_LIB) $(COVERAGE)
MAKE=make
ALL_OBJS=main/*.o base/*.o
INSTALL_DIR=/usr/local/bin
SUBDIRS := main base
HEADERS := $(wildcard main/*.h) $(wildcard base/*.h)
